import React from 'react';

function SelectAll() {
    const numbering = [
        {
            id: 1,
            name: '1',
            serveN: '기계전사 v',
            ver: '1.4.1',
            sum1: '5,000,000 건',
            sum2: '1,000,000 원',
            sum3: '30,120,000 원',
            sum4: '10,812 명',
