        {
            id: 2,
            name: '2',
            serveN: '기계전사 v',
            ver: '1.4.1',
            sum1: '5,000,000 건',
            sum2: '1,000,000 원',
            sum3: '30,120,000 원',
            sum4: '10,812 명',
        },
        {
            id: 3,
            name: '3',
            serveN: '기계전사 v',
            ver: '1.4.1',
            sum1: '5,000,000 건',
            sum2: '1,000,000 원',
            sum3: '30,120,000 원',
        },
        {
            id: 4,
            name: '4',
            serveN: '기계전사 v',
            ver: '1.4.1',
            sum1: '5,000,000 건',
            sum2: '1,000,000 원',
            sum3: '30,120,000 원',
        }
    ]
    return (
        numbering.map( numbering =>
            <ul className="boardTxt">
                <li className="num">
                    {numbering.id}
                </li>
                <li className="serveN">
                    {numbering.serveN}
                </li>
                <li className="ver">
                    {numbering.ver}
                </li>
                <li className="sum1">
                    {numbering.sum1}
                </li>
                <li className="sum2">
                    {numbering.sum2}
                </li>
                <li className="sum3">
                    {numbering.sum3}
                </li>
                <li className="sum4">
                    {numbering.sum4}
                </li>
            </ul>
        )
    )
}


